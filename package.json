{
  "name": "lodex",
  "version": "0.9.22",
  "description": "Linked Open Data and ISTEX",
  "main": "index.js",
  "bin": {
    "lodex": "lodex"
  },
  "scripts": {
    "build": "browserify -d -t vueify components/list.js -p [minifyify --no-map] > public/assets/js/list.min.js",
    "build-dev": "browserify -d -t vueify components/list.js > public/assets/js/list.min.js",
    "lint": "eslint ext=.js,.vue components/ downloaders/ filters/ loaders/ routes/",
    "test": "mocha",
    "start": "./lodex",
    "insertversion": "sed -i \"s#\\(LODEX version \\)\\([\\.a-z0-9]\\+\\)#\\1${npm_package_version}#g\" public/about.html",
    "preversion": "npm run build && git add public/assets/js/list.min.js",
    "version": "npm run insertversion && git add public/about.html",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Inist-CNRS/lodex.git"
  },
