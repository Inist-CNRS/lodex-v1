{
  "name": "lodex",
  "version": "1.0.10",
  "description": "Linked Open Data Publishing System by ISTEX",
  "main": "app/index.js",
  "bin": {
    "lodex": "bin/lodex"
  },
  "scripts": {
    "build": "browserify -d -t vueify app/components/main.js -p [minifyify --no-map] > app/public/assets/js/bundle.min.js",
    "build-dev": "browserify -d -t vueify app/components/main.js > app/public/assets/js/bundle.min.js",
    "lint": "eslint --ext=.js,.vue ./app/",
    "start": "./bin/lodex",
    "insertversion": "sed -i \"s#\\(LODEX version \\)\\([\\.a-z0-9]\\+\\)#\\1${npm_package_version}#g\" app/views/about.html",
    "preversion": "npm run lint && npm run build && git add app/public/assets/js/bundle.min.js",
    "version": "npm run insertversion && git add app/views/about.html",
    "postversion": "git push && git push --tags",
    "apilint": "drafter -lu doc/rest.apib && drafter -lu doc/actions.apib",
    "apidoc": "aglio -i doc/rest.apib  -o app/public/api-doc-rest.html && aglio -i doc/actions.apib  -o app/public/api-doc-actions.html",
    "coverage": "istanbul cover ./node_modules/.bin/_mocha -- ",
    "test": "mocha -- 2> /dev/null"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Inist-CNRS/lodex.git"
  },
  "contributors": [
    {
      "name": "Fran√ßois Parmentier",
