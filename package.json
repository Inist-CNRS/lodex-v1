  },
  "homepage": "https://github.com/Inist-CNRS/lodex#readme",
  "dependencies": {
    "body-parser": "^1.15.2",
    "castor-core": "^8.5.0",
    "castor-load": "^4.6.0",
    "castor-load-csv": "latest",
    "castor-load-excel": "latest",
    "castor-load-jsoncorpus": "latest",
    "castor-load-nq": "latest",
    "castor-load-xml": "latest",
    "connect-slashes": "^1.3.1",
    "cors": "^2.7.1",
    "datamodel": "^1.5.1",
    "debug": "latest",
    "heartbeats": "^3.1.3",
    "htmlspecialchars": "^1.0.5",
    "jbj": "^7.1.1",
    "jbj-array": "^1.0.0",
    "jbj-nlp": "^1.1.0",
    "jbj-numerical": "^1.0.0",
    "jbj-parse": "^2.0.0",
    "jbj-rdfa": "^1.1.2",
